<SetScreenElectricity>:
    Label:
        text: "Выберете комплект"
        color: 0, 0, 0
        font_size: 60
        size_hint: 1, 0.4
        pos_hint: {"center_x": 0.5, "center_y": 0.92}

    BoxLayout:
        orientation: "horizontal"
        size_hint: 0.8, 0.8
        pos_hint: {"center_x": 0.5, "center_y": 0.5}
        spacing: 115

        RoundedButton:
            text: "Комплект 1"
            font_size: 20
            size_hint: 0.3, 0.9
            pos_hint: {"center_x": 0.5, "center_y": 0.5}
            on_press:
                app.root.current = "electricity"
                app.root.transition.direction = "left"

        RoundedButton:
            text: "Комплект 2"
            font_size: 20
            size_hint: 0.3, 0.9
            pos_hint: {"center_x": 0.5, "center_y": 0.5}
            on_press:
                app.root.current = "electricity"
                app.root.transition.direction = "left"
