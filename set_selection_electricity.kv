<SetScreenElectricity>:
    MDScreen:
        MDNavigationLayout:
            MDScreenManager:
                MDScreen:
                    MDTopAppBar:
                        title: "Выберите комплект"
                        orientation: "vertical"
                        pos_hint: {"center_x": 0.5, "center_y": 0.97}
                        left_action_items: [["arrow-left-bold-circle-outline", lambda x: root.go_back()]]
                        right_action_items: [["account", lambda x: root.open_account()]]
                        elevation: 1
                        md_bg_color: 168/255, 228/255, 160/255, 1

    BoxLayout:
        orientation: "horizontal"
        size_hint: 0.8, 0.8
        pos_hint: {"center_x": 0.5, "center_y": 0.5}
        spacing: 115

        RoundedButton:
            text: "Комплект 1"
            font_size: 20
            size_hint: 0.3, 0.9
            pos_hint: {"center_x": 0.5, "center_y": 0.5}
            on_press:
                root.change(1)
                app.root.current = "electricity"
                app.root.transition.direction = "left"

        RoundedButton:
            text: "Комплект 2"
            font_size: 20
            size_hint: 0.3, 0.9
            pos_hint: {"center_x": 0.5, "center_y": 0.5}
            on_press:
                root.change(2)
                app.root.current = "electricity"
                app.root.transition.direction = "left"
